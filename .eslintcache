    pokemon.stats.map(stat => {\n      switch (stat.stat.name) {\n        case 'hp':\n          status['hp'] = stat['base_stat'];\n          break;\n        case 'attack':\n          status['attack'] = stat['base_stat'];\n          break;\n        case 'defense':\n          status['defense'] = stat['base_stat'];\n          break;\n        case 'speed':\n          status['speed'] = stat['base_stat'];\n          break;\n        case 'special-attack':\n          status['specialAttack'] = stat['base_stat'];\n          break;\n        case 'special-defense':\n          status['specialDefense'] = stat['base_stat'];\n          break;\n        default:\n          break;\n      }\n    });\n  }\n\n  const showTypeChips = () => {\n    return pokemon.types.map(type => {\n      return ( <TypeChip type={type.type.name} /> )\n    })\n  }\n\n  return (\n    <>\n      {pokemon && (\n        <Card style={styles.card}>\n          <CardTitle pokemonNumber={pokemon.id} types={showTypeChips()} />\n          <CardHeader title={t(pokemonName)} titleTypographyProps={{variant: 'h2'}}/>\n            <CardContent>\n              <img src={pokemon.sprites.front_default} alt=\"\" style={styles.baseImage}/>\n              <StatusBar status={'HP'} type={pokemon.types[0].type.name} param={status.hp} />\n              <StatusBar status={'attack'} type={pokemon.types[0].type.name} param={status.attack} />\n              <StatusBar status={'defense'} type={pokemon.types[0].type.name} param={status.defense} />\n              <StatusBar status={'speed'} type={pokemon.types[0].type.name} param={status.speed} />\n              <StatusBar status={'specialAttack'} type={pokemon.types[0].type.name} param={status.specialAttack} />\n              <StatusBar status={'specialDefense'} type={pokemon.types[0].type.name} param={status.specialDefense} />\n            </CardContent>\n          <CardMedia>\n            <img src={pokemon.sprites.front_default} alt=\"\"/>\n            <img src={pokemon.sprites.back_default} alt=\"\"/>\n            <img src={pokemon.sprites.front_shiny} alt=\"\"/>\n            <img src={pokemon.sprites.back_shiny} alt=\"\"/>\n          </CardMedia>\n          <CardContent>\n            <Typography variant=\"h4\">{t('profile')}</Typography>\n          </CardContent>\n        </Card>\n      )}\n    </>\n  );\n}\n","/Users/asamimasaki/workspace/react-pokemon-app/src/components/pages/not-found.jsx",[],["99","100"],"/Users/asamimasaki/workspace/react-pokemon-app/src/components/util/header/index.jsx",[],"/Users/asamimasaki/workspace/react-pokemon-app/src/components/util/chip/index.jsx",[],"/Users/asamimasaki/workspace/react-pokemon-app/src/components/util/loading/index.jsx",[],["101","102"],"/Users/asamimasaki/workspace/react-pokemon-app/src/components/util/status-bar/index.jsx",[],"/Users/asamimasaki/workspace/react-pokemon-app/src/components/util/card-title/index.jsx",[],"/Users/asamimasaki/workspace/react-pokemon-app/src/i18n/index.js",[],{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","replacedBy":"106"},{"ruleId":"103","replacedBy":"107"},{"ruleId":"105","replacedBy":"108"},{"ruleId":"103","replacedBy":"109"},{"ruleId":"105","replacedBy":"110"},{"ruleId":"103","replacedBy":"111"},{"ruleId":"105","replacedBy":"112"},{"ruleId":"113","severity":1,"message":"114","line":26,"column":32,"nodeType":"115","endLine":26,"endColumn":36},{"ruleId":"116","severity":1,"message":"117","line":42,"column":28,"nodeType":"118","messageId":"119","endLine":42,"endColumn":30},{"ruleId":"103","replacedBy":"120"},{"ruleId":"105","replacedBy":"121"},{"ruleId":"103","replacedBy":"122"},{"ruleId":"105","replacedBy":"123"},"no-native-reassign",["124"],"no-negated-in-lhs",["125"],["124"],["125"],["124"],["125"],["124"],["125"],"react-hooks/exhaustive-deps","Assignments to the 'unmounted' variable from inside React Hook useEffect will be lost after each render. To preserve the value over time, store it in a useRef Hook and keep the mutable value in the '.current' property. O